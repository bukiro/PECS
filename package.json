    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --disable-host-check --live-reload=false",
        "watch": "ng serve --host 0.0.0.0 --disable-host-check",
        "build": "ng build --configuration=production --output-path=./build",
        "build-demo": "ng build --configuration=production --output-path=../build/docs --base-href=/PECS-Demo/",
        "publish": "ng build --configuration=production --output-path=./publish/src",
        "publish-win": "cd publish && nexe ./pecs.js -t win-x64-16.16.0 --build && cd ..",
        "publish-linux": "cd publish && nexe ./pecs.js -t linux-x64-16.16.0 --build && cd ..",
        "test": "jest",
        "lint": "ng lint",
        "lint-fix": "ng lint --fix",
        "e2e": "ng e2e"
    },
