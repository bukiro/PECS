{
    "name": "@bukiro/pecs",
    "version": "1.0.16",
    "author": {
        "name": "Arne Brandt"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bukiro/PECS.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/bukiro"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --disable-host-check --live-reload=false",
        "watch": "ng serve --host 0.0.0.0 --disable-host-check",
        "build": "ng build --configuration=production --output-path=./build",
        "build-demo": "ng build --configuration=production --output-path=../build/docs --base-href=/PECS-Demo/",
        "publish": "ng build --configuration=production --output-path=./publish/src",
        "publish-win": "cd publish && nexe ./pecs.js -t win-x64 --build && cd ..",
        "publish-linux": "cd publish && nexe ./pecs.js -t linux-x64 --build && cd ..",
        "test": "jest",
        "lint": "ng lint",
        "lint-fix": "ng lint --fix",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "@angular/animations": "^17.2.1",
        "@angular/cdk": "^17.2.0",
        "@angular/common": "^17.2.1",
        "@angular/compiler": "^17.2.1",
        "@angular/core": "^17.2.1",
        "@angular/forms": "^17.2.1",
        "@angular/localize": "^17.2.1",
        "@angular/platform-browser": "^17.2.1",
        "@angular/platform-browser-dynamic": "^17.2.1",
        "@angular/router": "^17.2.1",
        "@ng-bootstrap/ng-bootstrap": "^16.0.0",
        "@ngrx/store": "^17.1.0",
        "body-parser": "^1.20.2",
        "bootstrap": "^5.3.2",
        "bootstrap-icons": "^1.11.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "gzip-js": "^0.3.2",
        "md5": "^2.3.0",
        "mongodb": "^6.3.0",
        "node-json-db": "^2.3.0",
        "rpg-awesome": "^0.2.0",
        "rxjs": "^7.8.1",
        "ts-md5": "^1.3.1",
        "uuid": "^9.0.1",
        "zone.js": "~0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.2.0",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "^17.2.0",
        "@angular/compiler-cli": "^17.2.1",
        "@angular/language-service": "^17.2.1",
        "@ngneat/spectator": "^16.0.0",
        "@team23/eslint-config-team23-angular": "^17.0.1",
        "@team23/eslint-config-team23-ngrx": "^17.0.1",
        "@team23/eslint-config-team23-standard": "^8.3.0",
        "@team23/eslint-config-team23-ts": "^7.0.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.19",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "angular-cli-ghpages": "^1.0.7",
        "eslint": "^8.56.0",
        "eslint-plugin-jest": "^27.9.0",
        "fuzzy": "^0.1.3",
        "jest": "^29.7.0",
        "jest-preset-angular": "^14.0.3",
        "prettier": "^3.3.3",
        "stylelint": "^16.2.1",
        "ts-node": "^10.9.2",
        "typescript": "5.3"
    }
