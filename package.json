  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --live-reload=false",
    "build": "ng build --prod=true --optimization=false --outputPath=./build",
    "build-demo": "ng build --prod=true --optimization=false --outputPath=../build/docs --baseHref=/PECS-Demo/",
    "publish": "ng build --prod=true --optimization=false --outputPath=./publish/src",
    "publish-win": "cd publish && nexe ./pecs.js -t win-x64 && cd ..",
    "publish-linux": "cd publish && nexe ./pecs.js -t linux-x64 && cd ..",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
